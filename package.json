{
  "name": "scriptsqd",
  "version": "1.0.0",
  "scripts": {
    "postinstall": "cd functions && npm i",
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "dev:ssr": "ng run scriptsqd:serve-ssr",
    "serve:ssr": "node dist/scriptsqd/server/main.js",
    "build:ssr": "ng build && ng run scriptsqd:server",
    "build:ssr-firebase": "npm run build:ssr && npm --prefix functions/ run build",
    "build:ssr-ci": "npm run build:ssr",
    "prerender": "ng run scriptsqd:prerender"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^15.0.3",
    "@angular/cdk": "^15.0.2",
    "@angular/common": "^15.0.3",
    "@angular/compiler": "^15.0.3",
    "@angular/core": "^15.0.3",
    "@angular/forms": "^15.0.3",
    "@angular/platform-browser": "^15.0.3",
    "@angular/platform-browser-dynamic": "^15.0.3",
    "@angular/platform-server": "^15.0.3",
    "@angular/router": "^15.0.3",
    "@angular/service-worker": "^15.0.3",
    "@nguniversal/express-engine": "^15.0.0",
    "@tailwindcss/typography": "^0.5.2",
    "express": "^4.18.1",
    "gsap": "^3.10.4",
    "rxjs": "~7.5.5",
    "shortid": "^2.2.16",
    "swiper": "^8.4.5",
    "tailwindcss": "^3.0.24",
    "tslib": "^2.4.0",
    "zone.js": "~0.11.5"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^15.0.3",
    "@angular/cli": "^15.0.3",
    "@angular/compiler-cli": "^15.0.3",
    "@nguniversal/builders": "^15.0.0",
    "@types/express": "^4.17.13",
    "@types/jasmine": "~4.0.3",
    "@types/node": "^17.0.36",
    "@types/shortid": "^0.0.29",
    "bufferutil": "^4.0.6",
    "jasmine-core": "~4.1.1",
    "karma": "~6.3.20",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.0.1",
    "karma-jasmine-html-reporter": "~2.0.0",
    "typescript": "~4.8.4",
    "utf-8-validate": "^5.0.9",
    "ws": "^8.7.0",
    "xhr2": "^0.2.1"
